--create TABLE
CREATE TABLE LINEA_DE_COLECTIVOS ( 
ID_CHOFERES INTEGER PRIMARY KEY AUTOINCREMENT,
ID_TURNOS TEXT,
UNIDADES NUMERIC
);


CREATE TABLE CHOFERES (
NOMBRE INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
DNI TEXT,
EDAD NUMERIC,
FECHA_INGRESO  datatime,
SEXO TEXT
ID_CHOFERES INTEGER NOT NULL,
APELLIDO TEXT,
INTERNO NUMERIC,
CORREO TEXT
);

CREATE TABLE UNIDADES (
INTERNO INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
PATENTE TEXT,
MODELO TEXT,
AIRE_ACONDICIONADO TEXT
ID_UNIDAD INTEGER NOT NULL
)


CREATE TABLE TURNO (
ID_TURNOS INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
NOMBRE TEXT,
INICIO NUMERIC,
FIN datatime
)




 FOREIGN key (ID_CHOFERES) REFERENCES LINEA_DE_COLECTIVOS(ID_CHOFERES)
 FOREIGN key (ID_TURNOS) REFERENCES LINEA_DE_COLECTIVOS(ID_CHOFERES)




